<manifest>
    <main-class>Creative_Seren_Miner</main-class>
    <name>Creative Seren Stone Miner</name>
    <tag-line>Creative</tag-line><!--Max of 50 chars-->
    <description>Creatively mines Seren Stone ore</description><!--Max of 110 chars-->
    <version>1.0.0</version>
    <compatibility>
        <game>RS3</game>
        <!--Can be used if you absolutely need to disable DarkScape (highly discouraged)-->
        <!--<disable-game>DARK</disable-game>-->
    </compatibility>
    <categories>
        <category>MINING</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>Creative Seren Stone Miner</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Seren</tag>
        <tag>Mine</tag>
    </tags>

</manifest>